{
  "name": "nav",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "nav/src",
  "prefix": "mfenx",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "nav/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["nav/**/*.ts", "nav/**/*.html"]
      }
    }
  }
}
